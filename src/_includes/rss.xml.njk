<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>CLCK's Space</title>
    <link>https://www.clckblog.space/</link>
    <description>Welcome to CLCK's Space!</description>
    <language>en-us</language>
    <lastBuildDate>{{ collections.blog[0].date | dateToRfc822 }}</lastBuildDate>

    {% for post in collections.blog %}
    <item>
      <title>{{ post.data.title }}</title>
      <link>https://www.clckblog.space{{ post.url }}</link>
      <description>{{ post.data.description }}</description>
      <pubDate>{{ post.date | dateToRfc822 }}</pubDate>
    </item>
    {% endfor %}
  </channel>
</rss>
